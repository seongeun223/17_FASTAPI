    "h, m = map(int, input().split())\n",
    "t = int(input())\n",
    "\n",
    "if (m + t) < 60:\n",
    "    print(h, m + t)\n",
    "else:\n",
    "    if h < 23:\n",
    "        print(h + ((m + t) // 60), (m + t) % 60)\n",
    "    else:\n",
    "        print((h + ((m + t) // 60)) % 24, (m + t) % 60)"
   ]
